{
    "name": "8fold/php-schema",
    "description": "8fold Schema is designed to make interacting with structured data for the web easier.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Bruce",
            "email": "josh@8fold.pro"
        }
    ],
    "require": {
        "php": "~7.0",
        "8fold/php-shoop-shelf": "~0.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.1"
    },
    "autoload": {
        "psr-4": {
            "Eightfold\\Schema\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eightfold\\Schema\\Tests\\": "./tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
	"minimum-stability": "dev",
	"prefer-stable": true
}
